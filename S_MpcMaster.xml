<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <serviceStateTable>  
	<stateVariable sendEvents="no">
       <name>bypass</name>
       <dataType>boolean</dataType>
       <defaultValue>0</defaultValue>
	   <shortCode>bypass</shortCode>
    </stateVariable>  	
	<stateVariable sendEvents="no">
       <name>mpcid1</name>
       <dataType>ui4</dataType>
       <defaultValue>222</defaultValue>
	   <shortCode>mpcid1</shortCode>
    </stateVariable>  
	<stateVariable sendEvents="no">
       <name>mpcid2</name>
       <dataType>ui4</dataType>
       <defaultValue>225</defaultValue>
	   <shortCode>mpcid2</shortCode>
    </stateVariable> 	
	<stateVariable sendEvents="no">
       <name>boundary</name>
       <dataType>ui4</dataType>
       <defaultValue></defaultValue>
	   <shortCode>boundary</shortCode>
    </stateVariable>  	
	<stateVariable sendEvents="no">
       <name>samplePeriod</name>
       <dataType>ui4</dataType>
       <defaultValue>15</defaultValue>
	   <shortCode>samplePeriod</shortCode>
    </stateVariable> 	
	<stateVariable sendEvents="no">
       <name>setpoint</name>
       <dataType>r8</dataType>
       <defaultValue>21</defaultValue>
	   <shortCode>setpoint</shortCode>
    </stateVariable> 	
  </serviceStateTable>
  <actionList>
    <action>
      <name>setMpcs</name>
	  <argumentList>
		<argument>
          <name>xId1</name>
          <direction>in</direction>
          <relatedStateVariable>mpcid1</relatedStateVariable>
        </argument>
		<argument>
          <name>xId2</name>
          <direction>in</direction>
          <relatedStateVariable>mpcid2</relatedStateVariable>
        </argument>	
		<argument>
          <name>xBoundary</name>
          <direction>in</direction>
          <relatedStateVariable>boundary</relatedStateVariable>
        </argument>		
		<argument>
          <name>xSamplePeriod</name>
          <direction>in</direction>
          <relatedStateVariable>samplePeriod</relatedStateVariable>
        </argument>		
      </argumentList>
	</action>
    <action>
      <name>setBypass</name>  
    </action>	
    <action>
      <name>unsetBypass</name>  
	  <argumentList>
		<argument>
          <name>xSetpoint</name>
          <direction>in</direction>
          <relatedStateVariable>setpoint</relatedStateVariable>
        </argument>		
      </argumentList>	  
    </action>		
    <action>
      <name>setSetpoint</name>  
	  <argumentList>
		<argument>
          <name>xSetpoint</name>
          <direction>in</direction>
          <relatedStateVariable>setpoint</relatedStateVariable>
        </argument>		
      </argumentList>	  
    </action>		
  </actionList>
</scpd>
